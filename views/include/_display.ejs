<% for(let i =0; i < users.length; i++){ %>
    <tr>
        <td><%= i+1 %></td>
        <td><%= users[i].lastname %></td>
        <td><%= users[i].firstname %></td>
        <td><%= users[i].email %></td>
        <td><%= users[i].gender %></td>
        <td><%= users[i].status %></td>
        <td>
            <a href="update-user?id=<%= users[i]._id %>" class="border-shadow">
                <span class="gradient-color"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a href="" class="border-shadow">
                <span class="gradient-color"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
<% } %>